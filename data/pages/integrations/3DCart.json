    {
      "title": "Add DHL as a shipping option with an OptimalShip neutral account #",
      "image": {
        "url": "/images/integrations/3d_cart/3dcart_1.png",
        "alt": "Add DHL",
        "width": 268,
        "height": 248
      },
      "content": {
        "orderedList": [
          "In your 3dcart admin panel click on \"Settings\"",
          "Click on Shipping",
          "In the Shipping Settings section click on \"Edit Settings\"",
          "Click Enter Credentials for DHL",
          "Enter DHL Neutral Account # for OptimalShip",
          "Click Save"
        ],
        "images": {
