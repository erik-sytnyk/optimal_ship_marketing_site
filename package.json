{
  "name": "optimal-shipp",
  "version": "0.1.0",
  "engines": {
    "node": "16.x"
  },
  "private": true,
  "scripts": {
    "dev": "react-env --path .env -- next dev",
    "build": "next build",
    "start": "npm run start-qa",
    "start-qa": "react-env --path .env.qa -- next start -p $PORT",
    "start-production": "react-env --path .env.production -- next start -p $PORT",
    "lint": "next lint",
    "format": "prettier \"**/*.{js,json,css,scss,jsx}\" --write"
  },
  "dependencies": {
    "@beam-australia/react-env": "^3.1.1",
    "next": "12.1.6",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.52.3",
    "sharp": "^0.30.7",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "eslint": "8.18.0",
    "eslint-config-next": "12.1.6",
    "typescript": "4.7.4"
  }
}
